{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/SAFE-Dashboard/(.*)",
      "destination": "/SAFE-Dashboard/$1"
    }
  ],
  "env": {
    "SAFE_ADDRESS": "@safe_address",
    "REOWN_API_KEY": "@reown_api_key"
  }
}
