# import os

# Define base project structure
base_path = "/mnt/data/SafeWalletApp"
os.makedirs(base_path, exist_ok=True)

# File structure to include in the zip
file_structure = {
    "pages/dashboard.tsx": dashboard_txs,
    "pages/settings.tsx": settings_page_code,
    "pages/propose.tsx": propose_page_code,
    "pages/queue.tsx": queue_page_code,
    "components/OwnerTools.tsx": owner_tools_code,
    "components/ProposalQueue.tsx": proposal_queue_code,
    "components/SignerControls.tsx": signer_controls_code,
    "components/SignTransactionButton.tsx": sign_button_code,
    "components/TransactionStatusPanel.tsx": status_panel_code,
    "hooks/useSafeWallet.ts": use_safe_wallet_code,
    "lib/safeApi.ts": safe_api_code,
}

# Write all files to disk
for path, content in file_structure.items():
    full_path = os.path.join(base_path, path)
    os.makedirs(os.path.dirname(full_path), exist_ok=True)
    with open(full_path, "w") as f:
        f.write(content)
